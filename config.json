{
  "name": "Lidarr",
  "version": "0.1.1",
  "slug": "lidarr",
  "legacy": false,
  "init": false,
  "maintenance": {
    "github_release": "https://github.com/linuxserver/docker-lidarr",
    "version_regex": "(\\d+\\.\\d+\\.\\d+\\.\\d+)-(ls\\d+)"
  },
  "description": "A fork of Sonarr to work with movies Ã  la Couchpotato",
  "url": "https://github.com/PrimusNZ/addon-lidarr",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "8686/tcp": 8686
  },
  "ports_description": {
    "8686/tcp": "web interface"
  },
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8686]",
  "boot": "auto",
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  },
  "backup_exclude": [
    "**/Backups/*",
    "**/logs/*",
    "**/MediaCover/*"
  ],
  "image": "ghcr.io/primusnz/hassio-addons/{arch}-lidarr"
}
